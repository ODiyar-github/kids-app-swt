{
    "name": "backend",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./mvnw clean package",
          "cwd": "apps/backend"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "./mvnw spring-boot:run",
          "cwd": "apps/backend"
        }
      }
    }
  }